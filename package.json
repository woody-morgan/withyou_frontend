{
  "name": "withyou_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "concurrently \"cross-env NODE_ENV=development node server.js\"",
    "dev:secure": "concurrently \"cross-env NODE_ENV=production ENABLE_HTTPS=true node server.js\" \"yarn test:dev\"",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:dev": "jest --watch",
    "test:build": "jest",
    "lint": "eslint --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ],
    "src/**": [
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.2.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
